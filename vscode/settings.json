{
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "markdownlint.run": "onSave",
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.detectIndentation": true,
  "editor.fontFamily": "Berkeley Mono, Menlo, Monaco, 'Courier New', monospace",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "commands": ["undo"],
      "before": ["u"]
    },
    {
      "commands": ["redo"],
      "before": ["<C-r>"]
    }
  ],
  "security.workspace.trust.untrustedFiles": "open",
  "redhat.telemetry.enabled": true,
  "vim.useCtrlKeys": true,
  "editor.accessibilitySupport": "off",
  "workbench.colorTheme": "Min Dark",
  "remote.SSH.remotePlatform": {"coder.c2":"linux","coder.ab":"linux","coder.moria":"linux","coder.c":"linux","coder-vscode--ammar--c":"linux","coder-vscode--ammar--c0":"linux","coder-vscode--ammar--powershell":"windows"},
  "gopls": {
    "ui.semanticTokens": true,
    "formatting.gofumpt": true
  },
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "workbench.iconTheme": "vscode-icons",
  "terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],

  "editor.rulers": [80],
  "editor.fontLigatures": true,

  "vsicons.dontShowNewVersionMessage": true,
  "remote.SSH.connectTimeout": 60,
  "remote.SSH.useLocalServer": true,
  "terminal.integrated.inheritEnv": false,
  "evenBetterToml.formatter.arrayAutoExpand": true,
  "diffEditor.codeLens": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.defaultProfile.linux": "fish",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js, ${capture}.typegen.ts",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts, ${capture}.typegen.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
    "*.mts": "${capture}.typegen.ts",
    "*.cts": "${capture}.typegen.ts"
  },
  "editor.renderWhitespace": "all",
  "shellformat.effectLanguages": [
    "shellscript",
    "dotenv",
    "hosts",
    "jvmoptions",
    "ignore",
    "gitignore",
    "properties",
    "spring-boot-properties",
    "azcli",
    "bats"
  ],
  "[dockerfile]": {
    "editor.formatOnSave": false
  },
  "git.mergeEditor": true,
  // "editor.inlineSuggest.enabled": true,
  "go.coverageOptions": "showUncoveredCodeOnly",
  "go.coverOnTestPackage": false,
  "terminal.integrated.localEchoEnabled": "off",
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-m>"],
      "after": ["@", "@"]
    }
  ],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabCompletion": "off",
  "editor.snippetSuggestions": "top",
  "[go]": {
    // "editor.suggest.snippetsPreventQuickSuggestions": true,
    // "editor.quickSuggestions": {
    //   "strings": "on"
    // }
  },
  "emmet.showSuggestionsAsSnippets": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlineSuggest.enabled": true,
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "go.gocodeAutoBuild": true,
  "go.buildTags": "mage",
  "remote.SSH.logLevel": "trace",
  "remote.SSH.showLoginTerminal": true,
  "julia.symbolCacheDownload": true,
  "julia.enableTelemetry": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": true
  },
  "notebook.formatOnSave.enabled": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "jupyter.interactiveWindow.codeLens.enable": true,
  "githubPullRequests.pullBranch": "never",
  "terminal.integrated.macOptionIsMeta": true,
  "extensions.ignoreRecommendations": true,
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "**/.sl": true
  },
  "window.zoomLevel": 1
}
