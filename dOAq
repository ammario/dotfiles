[32m+[m[32m# Put the line below in ~/.bashrc or ~/bash_profile:[m
[32m+[m[32m#[m
[32m+[m[32m#   eval "$(jump shell bash)"[m
[32m+[m[32m#[m
[32m+[m[32m# The following lines are autogenerated:[m
[32m+[m
[32m+[m[32m__jump_prompt_command() {[m
[32m+[m[32m  local status=$?[m
[32m+[m[32m  jump chdir && return $status[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m[[ "$PROMPT_COMMAND" =~ __jump_prompt_command ]] || {[m
[32m+[m[32m  PROMPT_COMMAND="__jump_prompt_command;$PROMPT_COMMAND"[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mj() {[m
[32m+[m[32m  local dir="$(jump cd $@)"[m
[32m+[m[32m  test -d "$dir"  && cd "$dir"[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mcomplete -o dirnames -C 'jump hint "${COMP_LINE/#j /}" --smart' j[m

