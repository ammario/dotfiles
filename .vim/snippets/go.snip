snippet rne
abbr require.NoError(t, err)
options head
    require.NoError(t, err)
    ${1}

snippet trun
options head
    t.Run("${1:TestName}", func(t *testing.T){
        ${2}
    })

snippet     ien
options     head
    if err != nil {
      ${1}
    }

snippet     iene
options     head
    if err != nil {
      return ${1}, err
    }

snippet     ewr
options     word
    errors.Wrapf(err, "${1}")
